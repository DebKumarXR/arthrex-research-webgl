{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,

		"strict": true,
		"skipLibCheck": true,
		"baseUrl": ".",
		"noEmit": true,
		"noUnusedParameters": true,
		"strictNullChecks": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
